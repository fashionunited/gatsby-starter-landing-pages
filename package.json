    "jest": "^21.2.1",
    "prettier": "^1.9.2",
    "react-dfp": "^0.6.0",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "write-good": "^0.11.3"
  },
  "keywords": ["gatsby"],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "node_modules/.bin/gatsby develop",
    "develop:aws": "node_modules/.bin/gatsby develop -p $PORT -H $IP",
    "dev": "node_modules/.bin/gatsby develop",
    "build": "node_modules/.bin/gatsby build",
    "serve": "node_modules/.bin/gatsby serve",
    "build:pp": "node_modules/.bin/gatsby build --prefix-paths",
    "build:gh": "npm run build:pp && gh-pages -d public",
    "build:fire": "node_modules/.bin/gatsby build && firebase deploy",
    "build:material":
      "node_modules/.bin/gatsby build && firebase deploy -P material-components-web",
    "heroku-postbuild": "gatsby build",
    "clean": "rm -rf public",
    "lint": "eslint --ext .js,.jsx .",
    "lint-fix": "eslint --ext .js,.jsx --fix .",
    "lint:md": "remark src/pages/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format": "prettier 'src/**/*.{js,jsx}' --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "remarkConfig": {
    "plugins": ["remark-preset-lint-recommended"]
  }
}
