{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "prettier"
  ],
  "globals": {
    "graphql": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 80
      }
    ],
    "prettier/prettier": [
      2
    ],
    "no-console": [
      2
    ],
    "import/extensions": [
      0
    ]
  }
}